<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CombinedTextView">
        <attr format="string" name="textPrimary"/>
        <attr format="string" name="textSecondary"/>
        <attr format="color" name="textPrimaryColor"/>
        <attr format="color" name="textSecondaryColor"/>
        <attr format="dimension" name="textPrimarySize"/>
        <attr format="dimension" name="textSecondarySize"/>
